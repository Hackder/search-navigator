{
  "manifest_version": 3,
  "name": "Search navigator",
  "version": "1.5.0",
  "description": "Let you navigate search results with shortcut keys",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  }
}
