{
  "manifest_version": 3,
  "name": "Search navigator",
  "version": "1.2",
  "description": "Let you navigate search results with shortcut keys",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"]
}
